{
  "name": "client",
  "version": "1.0.0",
  "description": "",
  "main": "client.js",
  "bin": "client.js",
  "pkg": {
    "assets": [],
    "scripts": [],
    "compress": "Brotli",
    "targets": [
      "node16-linux-x64",
      "node16-win-x64"
    ],
    "patches": {
      "util/types": "pkg-externals.js",
      "punycode": "pkg-externals.js",
      "diagnostics_channel": "pkg-externals.js",
      "async_hooks": "pkg-externals.js",
      "trace_events": "pkg-externals.js"
    }
  },
  "scripts": {
    "start": "node client.js",
    "build": "npm run build:win && npm run build:linux",
    "build:linux": "pkg . --targets node16-linux-x64 --output dist/chat-client-linux",
    "build:win": "pkg . --targets node16-win-x64 --output dist/chat-client.exe"
  },
